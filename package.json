{
  "name": "wikirace",
  "version": "1.0.0",
  "description": "Race from one Wikipedia page to another as fast as you can!",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && babel -d ./build ./src -s",
    "start": "npm run build && nodemon ./src/index.js | bunyan",
    "clean": "rm -rf ./build && mkdir build"
  },
  "author": "Phaedrus Raznikov",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "connect-mongo": "^2.0.3",
    "cookie-parser": "^1.4.4",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "module-alias": "^2.2.0",
    "mongoose": "^5.4.11",
    "nodemon": "^1.18.10",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "request": "^2.88.0"
  },
  "_moduleAliases": {
    "@root": "src",
    "@models": "src/models",
    "@routes": "src/routes"
  }
}
